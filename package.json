{
  "name": "ui-intshp2-cp-tarsiers",
  "homepage": "https://griddynamics.github.io/ui-intshp2-cp-tarsiers",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@babel/runtime": "^7.2.0",
    "axios": "^0.18.0",
    "babel-core": "^7.0.0-bridge.0",
    "body-parser": "^1.18.3",
    "cors": "^2.8.5",
    "express": "^4.16.4",
    "mongojs": "^2.6.0",
    "nodemon": "^1.18.9"
  },
  "devDependencies": {
    "husky": "^1.3.1",
    "lint-staged": "^8.1.4",
    "prettier": "^1.16.4"
  },
  "scripts": {
    "dev": "concurrently \"npm start\" \"cd client && npm start\"",
    "start": "node server",
    "heroku-postbuild": "cd client && npm install && npm run build"
  },
  "engines": {
    "node": "10.x"
  },
  "cacheDirectories": [
    "node_modules",
    "client/node_modules"
  ],
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": " cd client && npm run lint && npm run test --no-watch"
    }
  },
  "lint-staged": {
    "*.{js,json,css,scss,md}": [
      "prettier --write",
      "git add"
    ]
  }
}
